generator client {
  provider = "prisma-client-js"
  output   = "./generated/main"
}

datasource db1 {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model TestUser {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  firstName String? @map("first_name")
  lastName  String? @map("last_name")
  social    Json?
  isAdmin   Boolean @default(false) @map("is_admin")

  @@map("test_user")
}
